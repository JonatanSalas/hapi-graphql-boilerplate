{
  "name": "hapi-graphql-boilerplate",
  "version": "0.0.1",
  "license": "MIT",
  "description": "Hapi.js based Apollo GraphQL boilerplate",
  "bugs": {
    "url": "https://github.com/mike866/hapi-graphql-boilerplate/issues"
  },
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "start-dev": "npm run lint && npm run flow-check && nodemon index.js",
    "flow-check": "node_modules/.bin/flow check",
    "lint": "eslint . --ext .js"
  },
  "author": "Mike Chudziak (mike866)",
  "homepage": "https://github.com/mike866/hapi-graphql-boilerplate#readme",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/mike866/hapi-graphql-boilerplate.git"
  },
  "keywords": [
    "hapi",
    "graphql",
    "apollo",
    "server",
    "boilerplate"
  ],
  "dependencies": {
    "bluebird": "^3.4.6",
    "glue": "^4.0.0",
    "good": "^7.0.2",
    "good-console": "^6.1.2",
    "good-squeeze": "^5.0.0",
    "hapi": "^15.1.1",
    "hapi-auth-jwt2": "^7.1.3"
  },
  "devDependencies": {
    "babel-preset-node6": "^11.0.0",
    "babel-preset-stage-3": "^6.17.0",
    "babel-register": "^6.16.3",
    "blipp": "^2.3.0",
    "eslint": "^3.7.1",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-plugin-import": "^2.0.1",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "flow-bin": "^0.33.0",
    "nodemon": "^1.11.0"
  },
  "babel": {
    "presets": ["stage-3", "node6"]
  },
  "eslintConfig": {
    "extends": "airbnb/base",
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module"
    },
    "env": {
      "es6": true,
      "node": true
    },
    "rules": {
      "import/no-extraneous-dependencies": ["error", {"devDependencies": true}],
      "import/prefer-default-export": 0
    }
  }
}
